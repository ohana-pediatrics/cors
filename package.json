{
  "name": "@ahanapediatrics/cors",
  "version": "4.0.1",
  "description": "Cross-Origin Resource Sharing(CORS) for koa",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "release": "standard-version",
    "test": "NODE_ENV=test mocha --opts test/mocha.opts",
    "test-cov": "NODE_ENV=test nyc mocha --opts test/mocha.opts",
    "ci": "npm run lint && npm run test-cov",
    "lint": "tslint --project tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint"
  },
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "koa": "^2.11.0"
  },
  "dependencies": {
    "vary": "^1.1.2"
  },
  "devDependencies": {
    "@types/koa": "^2.11.0",
    "@types/mocha": "^8.0.2",
    "@types/supertest": "^2.0.8",
    "cz-conventional-changelog": "^3.0.2",
    "git-cz": "^3.3.0",
    "istanbul": "*",
    "koa": "^2.11.0",
    "mocha": "^7.0.1",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.4",
    "tslint": "^6.0.0",
    "typescript": "^3.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "homepage": "https://github.com/ohanapediatrics/cors",
  "repository": {
    "type": "git",
    "url": "git://github.com/ohanapediatrics/cors.git"
  },
  "bugs": {
    "url": "https://github.com/ohanapediatrics/cors/issues"
  },
  "keywords": [
    "cors",
    "Cross-Origin Resource Sharing",
    "koa",
    "koajs"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "author": "fengmk2 <fengmk2@gmail.com> (http://fengmk2.com)",
  "license": "MIT",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
